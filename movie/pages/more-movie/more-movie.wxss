/* pages/more-movie/more-movie.wxss */
@import '../../templates/movie-templates/movie-templates.wxss';

/* 列表布局 */
.movie_list{
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
}
.movie_item{
  width: 200rpx;
  margin:20rpx 0 0 37.5rpx ;

}
.movie_item .movie_img{
  width: 200rpx;
}
image{
  width: 100%;
  height: 100%;
}
.movie_scroll{
  position: fixed;
  top: 100rpx;
}
